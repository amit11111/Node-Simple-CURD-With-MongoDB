{
  "rules": {
    // "no-console": 0,
    "indent": "off",
    "import/no-dynamic-require": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": ["error", { "variables": false }],
    "linebreak-style": 0,
    "no-tabs": 0,
    "class-methods-use-this": 0,
    "camelcase": "off",
    "prefer-destructuring": ["error", {"object": false, "array": false}]
  },
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 9,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "extends": [
    "airbnb-base"
  ]
}
